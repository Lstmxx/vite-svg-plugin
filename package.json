{
  "name": "vite-svg-plugin",
  "version": "1.0.3",
  "description": "transfer svg file to svg symbol in vite",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "postbuild": "tsc  --emitDeclarationOnly --declaration --project tsconfig.json --outDir dist/esm",
    "dev": "npm run build && cd ./examples/vite && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lstmxx/vite-svg-plugin.git"
  },
  "keywords": [
    "vite",
    "svg",
    "svg-symbol",
    "plugin"
  ],
  "author": "Lstmxx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lstmxx/vite-svg-plugin/issues"
  },
  "homepage": "https://github.com/Lstmxx/vite-svg-plugin#readme",
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "esbuild": "^0.14.49",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-markdown": "^2.2.0",
    "rollup": "^2.77.0",
    "rollup-plugin-esbuild": "^4.9.1",
    "typescript": "^4.7.4",
  },
  "peerDependencies": {
    "vite": "^3.0.2 || ^4.0.0"
  },
}